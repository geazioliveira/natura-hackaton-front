extends ../layout

block content
    section.content-header
        h1
            | KPI
            small Quantidade de pessoas presentes nos eventos
        ol.breadcrumb
            li
                a(href='#')
                    i.fa.fa-dashboard
                    |  Home
            li
                a(href='#')
                    i.fa.fa-pie-chart
                    | KPI's
            li.active
                i.fa.fa-id-card-o
                |  Quantidade


    section.content
        .row
            .col-md-12
                .box
                    .box-header.with-border
                        h3.box-title Quantidade de CNs presente por ciclo
                    .box-body
                        div(style='width: 100%; height: 500px').quantity



block scripts
    script(src='/bower_components/echarts/echarts.min.js')
    script(src='/js/pages/quantity.js')
